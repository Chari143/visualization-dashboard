generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Record {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  end_year    String?
  intensity   Int?
  sector      String?
  topic       String?
  insight     String?
  url         String?
  region      String?
  start_year  String?
  impact      String?
  added       String?
  published   String?
  country     String?
  relevance   Int?
  pestle      String?
  source      String?
  title       String?
  likelihood  Int?
  swot        String?
  city        String?
  year        Int?
}
